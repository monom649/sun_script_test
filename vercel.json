{
  "functions": {
    "api/search.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/search",
      "destination": "/search.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
