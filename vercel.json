{
  "version": 2,
  "name": "sunsun-auth-search-v2",
  "functions": {
    "api/search.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/search",
      "dest": "/search.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
